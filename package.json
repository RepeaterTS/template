{
  "name": "@redstonets/template",
  "version": "0.0.1",
  "description": "an example template",
  "main": "dist/src/index.js",
  "scripts": {
    "prepublishOnly": "yarn build",
    "build": "tsc",
    "start": "node dist/app.js",
    "debug": "ts-node src/app.js",
    "test": "ava",
    "test:lint": "eslint --ext ts src test",
    "test:coverage": "npx nyc check-coverage --lines 90 --functions 90 --branches 90",
    "coverage": "npx nyc --require source-map-support/register npm test",
    "coverage:report": "npx nyc report --reporter=html",
    "lint": "eslint --fix --ext ts src test",
    "docs": "typedoc"
  },
  "license": "MIT",
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "ava": "^3.15.0",
    "eslint": "^7.18.0",
    "eslint-config-kashall": "Kashalls/kashall-lint",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.19",
    "typescript": "^4.1.3"
  },
  "ava": {
    "files": [
      "test/**/*.ts",
      "!test/lib"
    ],
    "typescript": {
      "extensions": [
        "ts"
      ],
      "rewritePaths": {
        "test/": "dist/test/"
      }
    }
  },
  "files": [
    "dist/src/"
  ],
  "engines": {
    "node": ">=10.1.0"
  }
}
